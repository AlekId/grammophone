<%
  function formatClassification(cs, c, n) {
    if (cs[c].member)
      return "The grammar is " + n + ".";
    else
      return "<span class=\"conflict\">Not " + n + " &mdash; " + cs[c].reason + ".</span></td>";
  }
%>

